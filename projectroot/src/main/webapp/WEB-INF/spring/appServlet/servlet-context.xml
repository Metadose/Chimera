                <beans:value>com.cebedo.pmsys.model.assignment.ManagerAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.StaffTeamAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.StaffFieldAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.StaffDeliveryAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.Team</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.TeamAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.Task</beans:value>
                <beans:value>com.cebedo.pmsys.model.ProjectFile</beans:value>
                <beans:value>com.cebedo.pmsys.model.SystemConfiguration</beans:value>
                <beans:value>com.cebedo.pmsys.model.Photo</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.TaskTeamAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.TaskStaffAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.assignment.TaskFieldAssignment</beans:value>
                <beans:value>com.cebedo.pmsys.model.SystemUser</beans:value>
                <beans:value>com.cebedo.pmsys.model.Company</beans:value>
                <beans:value>com.cebedo.pmsys.model.SecurityAccess</beans:value>
                <beans:value>com.cebedo.pmsys.model.SecurityRole</beans:value>
                <beans:value>com.cebedo.pmsys.model.AuditLog</beans:value>
                <beans:value>com.cebedo.pmsys.model.Milestone</beans:value>
                <beans:value>com.cebedo.pmsys.model.DeliveryToDelete</beans:value>
                <beans:value>com.cebedo.pmsys.model.Reminder</beans:value>
